{
  "name": "TodoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=developmentDebug",
    "ios": "react-native run-ios --scheme 'TodoAppDev' --configuration DebugDev",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android:dev": "react-native run-android --variant=developmentDebug",
    "android:dev-release": "react-native run-android --variant=developmentRelease",
    "android:stg": "react-native run-android --variant=stagingDebug",
    "android:stg-release": "react-native run-android --variant=stagingRelease",
    "android:prod": "react-native run-android --variant=productionDebug",
    "android:prod-release": "react-native run-android --variant=productionRelease",
    "ios:dev": "react-native run-ios --scheme 'TodoAppDev' --configuration DebugDev",
    "ios:dev-release": "react-native run-ios --scheme 'TodoAppDev' --configuration DebugRelease",
    "ios:stg": "react-native run-ios --scheme 'TodoAppStg' --configuration DebugDev",
    "ios:stg-release": "react-native run-ios --scheme 'TodoAppStg' --configuration DebugRelease",
    "ios:prod": "react-native run-ios --scheme 'TodoAppProd' --configuration DebugDev",
    "ios:prod-release": "react-native run-ios --scheme 'TodoAppProd' --configuration DebugRelease"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-config": "^1.4.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}